  <script>
    document.addEventListener('DOMContentLoaded', function () {
      var points = document.querySelectorAll('.point');
      var languageProjects = document.querySelectorAll('.language-projects');

      points.forEach(function (point, index) {
        point.addEventListener('click', function () {
          points.forEach(function (p) {
            p.classList.remove('active-point');
          });

          point.classList.add('active-point');

          languageProjects.forEach(function (project) {
            project.style.display = 'none';
          });

          var clickedLanguage = point.querySelector('.language').textContent.toLowerCase();
          document.getElementById(clickedLanguage + '-projects').style.display = 'block';

          var projectItems = document.querySelectorAll('#' + clickedLanguage + '-projects .project-item');
          projectItems.forEach(function (projectItem, projectIndex) {
            setTimeout(function () {
              projectItem.style.display = 'flex';
            }, projectIndex * 500);
          });

          setTimeout(function () {
            languageProjects.forEach(function (project) {
              project.style.display = 'none';
            });
            point.classList.remove('active-point');
          }, 10000);
        });
      });
    });
  

  

